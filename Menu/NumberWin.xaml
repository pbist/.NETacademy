<Window x:Class="ContactBook.NumberWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Tworzenie nowego kontaktu (4/4)" Height="300" Width="400" Closing="OnClose" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Podaj numer:" HorizontalAlignment="Left" Margin="24,99,0,0" VerticalAlignment="Top"/>
        <TextBox TextChanged="enableBtn" Name="textbox" HorizontalAlignment="Left" Height="23" Margin="109,102,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
        <Button Name="nextBtn" Content="Dodaj kontakt" HorizontalAlignment="Left" Margin="282,84,0,0" VerticalAlignment="Top" Width="90" Click="NextWindow"/>
        <Button Content="Wstecz" HorizontalAlignment="Left" Margin="282,129,0,0" VerticalAlignment="Top" Width="75" Click="PreviousWindow"/>
    </Grid>
</Window>
